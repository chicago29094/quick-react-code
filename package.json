{
  "name": "quick-react-code",
  "displayName": "quick-react-code",
  "description": "Start building React apps faster with Quick-React and VS Code.",
  "version": "0.5.0",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:quick-react-code.quickReact"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-react-code.quickReact",
        "title": "Quick-React"
      }
    ],
    "configuration": [
      {
        "title": "Quick-React",
        "properties": {
          "confirmOverwrite": {
            "type": "boolean",
            "default": true,
            "description": "Always manually confirm overwriting of existing files?"
          }
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
